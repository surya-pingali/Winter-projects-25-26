range=np.linspace(0,5,1000)\n","energy_results =[]\n","def energy(A):\n","    x=A * np.sin((np.pi * array)/10)\n","\n","    Vx=A * (np.pi/10) * np.cos(np.pi*array/10)\n","    Vy=1\n","    Mvelocity= np.sqrt(Vx**2+Vy**2)\n","    Fx=np.zeros(total_points)\n","    Fy=-2/(x+1)**2\n","    Rate_of_work=(Fx*Vx +Fy*Vy)\n","    E=alpha*Mvelocity - Rate_of_work\n","    Energy=np.sum(E)*dt\n","    return energy\n","for A_value in test_range:\n","    Energy=energy(A_value)\n","    Energy_results.append((A_value, Energy))\n","Energy_results_array=np.array(Energy_results)\n","\n","min_energy_index=np.argmin(Energy_results_array[:,1])\n","Optimal_A= Energy_results[min_energy_index, 0]\n","Min_energy= Energy_results[min_energy_index, 1]"],"metadata":{"id":"hBwzNm6myI3M","colab":{"base_uri":"https://localhost:8080/","height":211},"executionInfo":{"status":"error","timestamp":1765483587192,"user_tz":-330,"elapsed":803,"user":{"displayName":"Hema Gupta","userId":"11061382825843039538"}},"outputId":"69d90ec9-fa9a-4738-dfce-d25fd5ffbf61"},"execution_count":null,"outputs":[{"output_type":"error","ename":"AttributeError","evalue":"'numpy.ndarray' object has no attribute 'append'","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mAttributeError\u001b[0m                            Traceback (most recent call last)","\u001b[0;32m/tmp/ipython-input-1039397717.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     23\u001b[0m \u001b[0;32mfor\u001b[0m \u001b[0mA_value\u001b[0m \u001b[0;32min\u001b[0m \u001b[0mtest_range\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     24\u001b[0m     \u001b[0mEnergy\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0menergy\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mA_value\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 25\u001b[0;31m     \u001b[0mEnergy_results\u001b[0m\u00